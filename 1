#include <iostream>
#include <string>
using namespace std;
int main() {
	string s, a, q, w, r;
	r = "1";
	q = "qwertyuiopasdfghjklzxcvbnm1234567890";
	w = "QWERTYUIOPASDFGHJKLZXVCBNM";
	getline(cin, s);
	for (auto c : s) {
		if (q.find(c) != -1) {
			a.push_back(c);
		}
		else {
			if (q.find(c) != -1) {	
				a.push_back('a'+c-'A');
				}
			}
		}
	for (int i = 0; i < (a.size()%2); i++) {
		if (s[i] != s[s.size()-1-i]) {
			cout << "NO";
			r = "0";
			break;
		}
	}
	if (r != "0") {
		cout << "YES";
	}
}
